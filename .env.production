# Production Environment Variables
# These will be set in AWS Elastic Beanstalk Console

NODE_ENV=production
PORT=3000

# Database (current Prisma Accelerate setup)
DATABASE_URL="prisma+postgres://accelerate.prisma-data.net/?api_key=YOUR_API_KEY_HERE"

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ASSISTANT_ID=your_openai_assistant_id_here

# Auth0 Configuration
AUTH0_DOMAIN=your_auth0_domain_here
AUTH0_AUDIENCE=your_aws_app_url
AUTH0_CLIENT_SECRET=your_auth0_client_secret_here

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here
STRIPE_PRICE_ID_PREMIUM=your_stripe_price_id_here

# Application Configuration
FRONTEND_URL=your_frontend_url_here
ALLOWED_ORIGINS=your_frontend_urls_comma_separated